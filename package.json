{
  "name": "husky-lint-staged-nested",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "husky": "^4.2.5",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS  =~ 1$ ]]; then npm install --no-package-lock; fi",
      "post-merge": "npm install --no-package-lock",
      "post-rebase": "npm install --no-package-lock",
      "precommit:web-app": "cd web-app && npm run precommit"
    }
  }
}
