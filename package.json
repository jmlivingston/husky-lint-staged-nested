{
  "name": "husky-lint-staged-nested",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "husky": "^4.2.5",
    "lerna": "^3.22.1"
  },
  "husky": {
    "hooks": {
      "post-checkout": "if [[ $HUSKY_GIT_PARAMS  =~ 1$ ]]; then npm install --no-package-lock; fi",
      "post-merge": "npm install --no-package-lock",
      "post-rebase": "npm install --no-package-lock",
      "pre-commit": "lerna run --concurrency 1 --stream lint --since HEAD --exclude-dependents"
    }
  }
}
